<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IDataFeeds"  receiveTimeout="00:10:00"/>
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint 
              address="http://localhost:47094/DataFeedsService.svc"
              binding="webHttpBinding"
              behaviorConfiguration="DataFeeds"
              contract="DataFeedsService.IDataFeeds" 
              name="DataFeeds" />
        </client>
        <behaviors>
          <endpointBehaviors>
            <behavior name="DataFeeds">
              <webHttp/>
            </behavior>
          </endpointBehaviors>
      </behaviors>
    </system.serviceModel>
</configuration>